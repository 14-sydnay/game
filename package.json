{
  "name": "red-runner",
  "version": "1.0.0",
  "description": "Game implemented with Canvas API",
  "license": "MIT",
  "scripts": {
    "start": "http-server ./dist",
    "build": "npm run format && cross-env TS_NODE_PROJECT=\"tsconfig.root.json\" NODE_ENV=production webpack --config ./webpack.production.ts",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig.root.json\" NODE_ENV=development webpack-dev-server --config ./webpack.development.ts",
    "format": "prettier --write --config ./.prettierrc .",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build:server": "cross-env TS_NODE_PROJECT=\"./src/server/tsconfig.json\" webpack --config ./webpack/server.config.ts",
    "build:client": "cross-env TS_NODE_PROJECT=\"./src/client/tsconfig.json\" webpack --config ./webpack/client.config.ts",
    "build:all": "npm-run-all -c --print-label build:server build:client",
    "build:all:prod": "cross-env NODE_ENV=production npm-run-all -c --print-label build:server build:client",
    "start:server": "node ./dist/server.js",
    "docker:compose": "docker compose --env-file ./.env.dev up -d"
  },
